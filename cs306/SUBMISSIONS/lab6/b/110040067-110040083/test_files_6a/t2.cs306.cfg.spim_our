
	.text 			# The .text assembler directive indicates
	.globl main		# The following is the code (as oppose to data)
main:				# .globl makes main know to the 
				# outside of the program.
# Prologue begins 
	sw $fp, 0($sp)		# Save the frame pointer
	sub $fp, $sp, 4		# Update the frame pointer

	sub $sp, $sp, 24		# Make space for the locals
# Prologue ends


label2:
	lw $v0, -4($fp)
	lw $t0, 0($fp)
	sne $t1, $v0, $t0
	bne $t1, $zero, label3
	j label7

label3:
	lw $v0, -4($fp)
	lw $t0, 0($fp)
	add $t1, $v0, $t0
	li $v0, 0
	sne $t0, $t1, $v0
	bne $t0, $zero, label4
	j label5

label4:
	lw $v0, -4($fp)
	lw $t0, 0($fp)
	mul $t1, $v0, $t0
	sw $t1, -12($fp)
	j label6

label5:
	lw $v0, 0($fp)
	neg $t0, $v0
	sw $t0, -12($fp)

label6:
	lw $v0, -12($fp)
	sw $v0, -16($fp)
	j label8

label7:
	lw $v0, -4($fp)
	lw $t0, 0($fp)
	div $t1, $v0, $t0
	sw $t1, -16($fp)

label8:
	lw $v0, -16($fp)
	sw $v0, -8($fp)

# Epilogue Begins
	add $sp, $sp, 24
	lw $fp, 0($sp)  
	jr        $31		# Jump back to the operating system.
# Epilogue Ends

