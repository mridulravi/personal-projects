
	.text 			# The .text assembler directive indicates
	.globl main		# The following is the code (as oppose to data)
main:				# .globl makes main know to the 
				# outside of the program.
# Prologue begins 
	sw $fp, 0($sp)		# Save the frame pointer
	sub $fp, $sp, 4		# Update the frame pointer

	sub $sp, $sp, 20		# Make space for the locals
# Prologue ends


label2:
	li $v0, 5
	sw $v0, -12($fp)
	lw $v0, -12($fp)
	neg $t0, $v0
	sw $t0, -8($fp)
	lw $v0, -12($fp)
	li $t0, 2
	add $t1, $v0, $t0
	sw $t1, 0($fp)
	lw $v0, -12($fp)
	lw $t0, -8($fp)
	add $t1, $v0, $t0
	sw $t1, -4($fp)
	lw $v0, -12($fp)
	lw $t0, -8($fp)
	sub $t1, $v0, $t0
	sw $t1, -4($fp)
	lw $v0, -12($fp)
	lw $t0, -8($fp)
	mul $t1, $v0, $t0
	sw $t1, -4($fp)
	lw $v0, -12($fp)
	lw $t0, -8($fp)
	div $t1, $v0, $t0
	sw $t1, -4($fp)

# Epilogue Begins
	add $sp, $sp, 20
	lw $fp, 0($sp)  
	jr        $31		# Jump back to the operating system.
# Epilogue Ends

