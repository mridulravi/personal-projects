DROP TABLE IF EXISTS Castings;
DROP TABLE IF EXISTS MovieAwards;
DROP TABLE IF EXISTS Movies;
DROP TABLE IF EXISTS MoviePersons;
DROP TABLE IF EXISTS Users;
CREATE TABLE Users(username varchar(40) PRIMARY KEY, password varchar(40), role varchar(20));
